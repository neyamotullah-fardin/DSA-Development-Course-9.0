#include <iostream>
using namespace std;

void BinToDec(int bin) {
    int n = bin;
    int dec = 0;
    int pow = 1;
    while(n > 0) {
        int lastDig = n % 10;
        dec = dec + (lastDig * pow);
        pow = pow * 2;
        n = n / 10;

    }
    cout<<"Deciimal ("<<bin<<") = "<<dec;
}

int main() {
    BinToDec(111111);
    
    return 0;
}
